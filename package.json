{
  "name": "yarn-tree",
  "version": "1.1.0",
  "main": "index.js",
  "repository": "git@github.com:KnisterPeter/yarn-tree.git",
  "author": "Markus Wolf <mail@markus-wolf.de>",
  "license": "MIT",
  "bin": {
    "yarn-tree": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "node dist/cli",
    "build": "tsc -b ."
  },
  "dependencies": {
    "@yarnpkg/lockfile": "^1.1.0",
    "source-map-support": "^0.5.19",
    "yargs": "^17.0.0"
  },
  "devDependencies": {
    "@types/node": "16.11.7",
    "@types/yargs": "17.0.5",
    "@types/yarnpkg__lockfile": "1.1.5",
    "prettier": "2.4.1",
    "type-fest": "2.5.4",
    "typescript": "4.4.4"
  },
  "prettier": {},
  "renovate": {
    "extends": [
      "config:base"
    ],
    "lockFileMaintenance": {
      "enabled": true,
      "automerge": true
    },
    "packageRules": [
      {
        "depTypeList": [
          "devDependencies"
        ],
        "updateTypes": [
          "minor",
          "patch"
        ],
        "automerge": true
      },
      {
        "packagePatterns": [
          "^@types/"
        ],
        "automerge": true
      }
    ]
  }
}
